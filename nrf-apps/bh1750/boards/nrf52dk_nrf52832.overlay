&i2c0 { /* SDA P0.26, SCL P0.27, ALERT P1.11 */
	bh1750@23 {
		compatible = "rohm,bh1750";
		reg = <0x23>;
	};
};
/ {
	chosen {
		zephyr,console = &cdc_acm_uart0;
	};
};

&zephyr_udc0 {
	cdc_acm_uart0: cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
	};
};
